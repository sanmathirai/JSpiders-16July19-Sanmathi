package com.ustglobal.map;

import java.util.HashMap;

public class TestA {
	
	public static void main(String[] args) {
		
		HashMap hm = new HashMap();
		
		
		hm.put("Kuku", 456545);
		hm.put("Mala", 4545);
		hm.put("sheela", 556545);
		hm.put("Meena", 656545);

		System.out.println("Data of hashmap is :\n"+hm);//toString is overriden
		hm.put("Kuku", 787);
		System.out.println("data After adding duplicate key :\n"+hm);
		
		hm.put("Sanmathi", 656545);
		System.out.println("data After adding duplicate value :\n"+hm);
		
		
		hm.put(null, 787);
		System.out.println("---data After adding null key :--\n"+hm);
		
	}
}
