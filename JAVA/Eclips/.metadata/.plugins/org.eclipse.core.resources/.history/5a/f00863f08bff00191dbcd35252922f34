package com.ustgloabl.sorting.set;

import java.util.Iterator;
import java.util.TreeSet;

public class TestJ1 {
	TreeSet<Employee1> ts = new TreeSet<Employee1>();

	Employee1 e1 = new Employee1(1, "SAnmathi", 5.1);
	Employee1 e2 = new Employee1(2, "Radhika", 5.4);
	Employee1 e3 = new Employee1(3, "Megha", 4.9);
	Employee1 e4 = new Employee1(4, "Jhon", 5.11);
	
	ts.add(e1);
	ts.add(e2);
	ts.add(e3);
	ts.add(e4);
	
	System.out.println("----Using iterator=====");
	Iterator<Employee1> it = ts.iterator();
	while(it.hasNext()) {
		Customer b = it.next();

		System.out.println("Customer name :"+b.name);
		System.out.println("pincode is"+b.id);
		System.out.println("MICR"+b.salary);
		System.out.println("================");

	}
	
	
			
}
