package com.ustglobal.map;

import java.util.HashMap;

public class TestA {
	public static void main(String[] args) {
		HashMap hm = new HashMap();
		hm.put("Kuku", 456545);
		hm.put("Mala", 4545);
		hm.put("sheela", 556545);
		hm.put("Meena", 656545);

		System.out.println("Data of hashmap is :\n"+hm);
	}
}
